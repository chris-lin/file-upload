!!!
html
  head
    link(rel="stylesheet", href="stylesheets/bootstrap.min.css")
    link(rel="stylesheet", href="stylesheets/style.css")
    link(rel="stylesheet", href="stylesheets/bootstrap-responsive.min.css")
    link(rel="stylesheet", href="stylesheets/bootstrap-image-gallery.min.css")
    link(rel="stylesheet", href="stylesheets/jquery.fileupload-ui.css")
    title= title
  body
    form#fileupload(enctype="multipart/form-data", method="POST", action="localhost:8888")
        div(class="row fileupload-buttonbar")
            div.span7
                span(class="btn btn-success fileinput-button")
                    i(class="icon-plus icon-white")
                    span Add files...
                    input(type="file", multiple="", name="files[]")
                button(class="btn btn-primary start", type="submit")
                    i(class="icon-upload icon-white")
                    span Start upload
                button(class="btn btn-warning cancel", type="reset")
                    i(class="icon-ban-circle icon-white")
                    span Cancel upload
                button(class="btn btn-danger delete", type="button")
                    i(class="icon-trash icon-white")
                    span Delete
                input(class="toggle", type="checkbox")
            div(class="span5 fileupload-progress fade")
            div(class="progress progress-success progress-striped active", aria-valuemax="100", aria-valuemin="0", role="progressbar", aria-valuenow="0")
                div.progress-extended
        div(class="fileupload-loading") 
        table(class="table table-striped", role="presentation")
            tbody(class="files", data-target="#modal-gallery", data-toggle="modal-gallery")
    script#template-upload(type="text/x-tmpl")
    script#template-download(type="text/x-tmpl")
    script(src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js")
    script(src="javascripts/vendor/jquery.ui.widget.js")
    script(src="javascripts/tmpl.min.js")
    script(src="javascripts/load-image.min.js")
    script(src="javascripts/canvas-to-blob.min.js")
    script(src="javascripts/bootstrap.min.js")
    script(src="javascripts/bootstrap-image-gallery.min.js")
    script(src="javascripts/jquery.iframe-transport.js")
    script(src="javascripts/jquery.fileupload.js")
    script(src="javascripts/jquery.fileupload-fp.js")
    script(src="javascripts/jquery.fileupload-ui.js")
    script(src="javascripts/locale.js")
    script(src="javascripts/main.js")
